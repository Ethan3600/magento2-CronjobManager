<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <system>
            <cron_job_manager>
                <clean_running_schedule>1</clean_running_schedule>
                <email_notification>0</email_notification>
                <email_identity>general</email_identity>
                <email_recipients></email_recipients>
                <email_template>system_cron_job_manager_email_template</email_template>
                <retry_failed_jobs>0</retry_failed_jobs>
                <retry_jobs_gone_away>1</retry_jobs_gone_away>
            </cron_job_manager>
        </system>
    </default>
</config>
